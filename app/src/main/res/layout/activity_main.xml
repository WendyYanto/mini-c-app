<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hi"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_core_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="core text not loaded"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_data_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="data user text not loaded"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_data_product"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="data product text not loaded"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_data_order"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="data order text not loaded"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_data_misc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="data misc text not loaded"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_domain_cart"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="domain cart text not loaded"
        android:textSize="20sp" />
</LinearLayout>